@using SimpleApp.Models
@model IEnumerable<Product>

@{ 
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Simple App</title>
</head>
<body>
    <ul>
        @foreach (var product in Model)
        {
            <li>Name: @product.Name, Price: @product.Price</li>
        }
    </ul>
</body>
</html>